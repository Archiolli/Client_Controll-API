generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model processos {
  id       BigInt @id @default(autoincrement())
  nome     String @unique
  processo String
  email    String
  status   Int

  @@map("processos")
}

model User {
  id    BigInt @id @default(autoincrement())
  nome  String
  email String @unique
  senha String

  @@map("users")
}
